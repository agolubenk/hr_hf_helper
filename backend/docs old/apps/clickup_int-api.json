{
  "openapi": "3.0.0",
  "info": {
    "title": "ClickUp Integration API",
    "version": "1.0.0",
    "description": "API endpoints for ClickUp integration, task synchronization, bulk import, and Huntflow transfer in the HR Helper system."
  },
  "servers": [
    {
      "url": "/api/v1/clickup-int/"
    }
  ],
  "paths": {
    "/settings/": {
      "get": {
        "summary": "Get ClickUp Settings",
        "description": "Retrieve the ClickUp integration settings for the authenticated user.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "ClickUp settings for the user.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ClickUpSettings"
                }
              }
            }
          },
          "404": {
            "description": "Settings not found."
          }
        }
      },
      "post": {
        "summary": "Update ClickUp Settings",
        "description": "Update the ClickUp integration settings for the authenticated user.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ClickUpSettingsUpdate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Settings updated successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid settings data."
          }
        }
      }
    },
    "/settings/test-connection/": {
      "post": {
        "summary": "Test ClickUp Connection",
        "description": "Test the connection to ClickUp API using the user's API token.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Connection test result.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    },
                    "user_info": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/path-options/": {
      "get": {
        "summary": "Get Path Options",
        "description": "Retrieve available teams for ClickUp path configuration.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Available teams and path options.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "teams": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string"
                          },
                          "name": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/spaces/": {
      "get": {
        "summary": "Get Team Spaces",
        "description": "Retrieve spaces for a specific team.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "team_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "ID of the team to get spaces for."
          }
        ],
        "responses": {
          "200": {
            "description": "List of team spaces.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "spaces": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string"
                          },
                          "name": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/folders/": {
      "get": {
        "summary": "Get Space Folders",
        "description": "Retrieve folders for a specific space.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "space_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "ID of the space to get folders for."
          }
        ],
        "responses": {
          "200": {
            "description": "List of space folders.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "folders": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string"
                          },
                          "name": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/lists/": {
      "get": {
        "summary": "Get Task Lists",
        "description": "Retrieve task lists for a specific folder or space.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "folder_id",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "description": "ID of the folder to get lists for."
          },
          {
            "name": "space_id",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "description": "ID of the space to get lists for."
          }
        ],
        "responses": {
          "200": {
            "description": "List of task lists.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "lists": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string"
                          },
                          "name": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/tasks/": {
      "get": {
        "summary": "List ClickUp Tasks",
        "description": "Retrieve a paginated list of cached ClickUp tasks for the authenticated user with optional filtering and search.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "search",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "description": "Search query for task name, description, or status"
          },
          {
            "name": "status",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "description": "Filter by task status"
          },
          {
            "name": "priority",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "description": "Filter by task priority"
          },
          {
            "name": "sort",
            "in": "query",
            "schema": {
              "type": "string",
              "default": "-date_updated"
            },
            "description": "Sort order (e.g., '-date_updated', 'name', 'status')"
          },
          {
            "name": "page",
            "in": "query",
            "schema": {
              "type": "integer",
              "default": 1
            },
            "description": "Page number for pagination"
          }
        ],
        "responses": {
          "200": {
            "description": "A paginated list of ClickUp tasks.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "results": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/ClickUpTask"
                      }
                    },
                    "count": {
                      "type": "integer"
                    },
                    "next": {
                      "type": "string",
                      "nullable": true
                    },
                    "previous": {
                      "type": "string",
                      "nullable": true
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/tasks/{task_id}/": {
      "get": {
        "summary": "Retrieve a ClickUp Task",
        "description": "Retrieve detailed information about a specific ClickUp task by ID.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "task_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "ID of the ClickUp task."
          }
        ],
        "responses": {
          "200": {
            "description": "Details of the ClickUp task.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ClickUpTaskDetail"
                }
              }
            }
          },
          "404": {
            "description": "Task not found."
          }
        }
      }
    },
    "/tasks/{task_id}/transfer-to-huntflow/": {
      "post": {
        "summary": "Transfer ClickUp Task to Huntflow",
        "description": "Transfer data from a ClickUp task to Huntflow as a candidate.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "task_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "ID of the ClickUp task to transfer."
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ClickUpTransferRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Transfer completed successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    },
                    "applicant_id": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Transfer failed or invalid data."
          },
          "404": {
            "description": "Task not found."
          }
        }
      }
    },
    "/sync-tasks/": {
      "post": {
        "summary": "Sync ClickUp Tasks",
        "description": "Trigger synchronization of ClickUp tasks from the configured list.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Synchronization completed successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    },
                    "tasks_processed": {
                      "type": "integer"
                    },
                    "tasks_created": {
                      "type": "integer"
                    },
                    "tasks_updated": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Synchronization failed or settings incomplete."
          }
        }
      }
    },
    "/clear-cache/": {
      "post": {
        "summary": "Clear ClickUp Cache",
        "description": "Clear all cached ClickUp tasks for the authenticated user.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Cache cleared successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/bulk-import/": {
      "get": {
        "summary": "List Bulk Imports",
        "description": "Retrieve a list of bulk import operations for the authenticated user.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "schema": {
              "type": "string",
              "enum": ["running", "completed", "failed", "cancelled", "stopped"]
            },
            "description": "Filter by import status"
          }
        ],
        "responses": {
          "200": {
            "description": "A list of bulk imports.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ClickUpBulkImport"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Start Bulk Import",
        "description": "Start a bulk import operation for ClickUp tasks.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ClickUpBulkImportRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Bulk import started successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "import_id": {
                      "type": "integer"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid import parameters."
          }
        }
      }
    },
    "/bulk-import/{id}/": {
      "get": {
        "summary": "Retrieve Bulk Import",
        "description": "Retrieve details of a specific bulk import operation by ID.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            },
            "description": "ID of the bulk import."
          }
        ],
        "responses": {
          "200": {
            "description": "Details of the bulk import.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ClickUpBulkImport"
                }
              }
            }
          },
          "404": {
            "description": "Bulk import not found."
          }
        }
      },
      "post": {
        "summary": "Stop Bulk Import",
        "description": "Stop a running bulk import operation.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            },
            "description": "ID of the bulk import."
          }
        ],
        "responses": {
          "200": {
            "description": "Bulk import stopped successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Bulk import not found."
          }
        }
      }
    },
    "/bulk-import/{id}/progress/": {
      "get": {
        "summary": "Get Bulk Import Progress",
        "description": "Retrieve the progress of a specific bulk import operation.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            },
            "description": "ID of the bulk import."
          }
        ],
        "responses": {
          "200": {
            "description": "Bulk import progress details.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "integer"
                        },
                        "status": {
                          "type": "string"
                        },
                        "total_tasks": {
                          "type": "integer"
                        },
                        "processed_tasks": {
                          "type": "integer"
                        },
                        "successful_tasks": {
                          "type": "integer"
                        },
                        "failed_tasks": {
                          "type": "integer"
                        },
                        "progress_percentage": {
                          "type": "number"
                        },
                        "success_rate": {
                          "type": "number"
                        },
                        "failed_task_ids": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          }
                        },
                        "error_message": {
                          "type": "string"
                        },
                        "created_at": {
                          "type": "string",
                          "format": "date-time"
                        },
                        "updated_at": {
                          "type": "string",
                          "format": "date-time"
                        },
                        "completed_at": {
                          "type": "string",
                          "format": "date-time",
                          "nullable": true
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Bulk import not found."
          }
        }
      }
    },
    "/bulk-import/{id}/retry-failed/": {
      "post": {
        "summary": "Retry Failed Tasks",
        "description": "Retry import of failed tasks from a bulk import operation.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            },
            "description": "ID of the bulk import."
          }
        ],
        "responses": {
          "200": {
            "description": "Retry operation started successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Bulk import not found."
          }
        }
      }
    },
    "/sync-logs/": {
      "get": {
        "summary": "List Sync Logs",
        "description": "Retrieve a paginated list of synchronization logs for the authenticated user.",
        "security": [
          {
            "cookieAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "schema": {
              "type": "integer",
              "default": 1
            },
            "description": "Page number for pagination"
          }
        ],
        "responses": {
          "200": {
            "description": "A paginated list of sync logs.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "results": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/ClickUpSyncLog"
                      }
                    },
                    "count": {
                      "type": "integer"
                    },
                    "next": {
                      "type": "string",
                      "nullable": true
                    },
                    "previous": {
                      "type": "string",
                      "nullable": true
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ClickUpSettings": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "user": {
            "type": "integer",
            "description": "User ID"
          },
          "user_username": {
            "type": "string",
            "readOnly": true,
            "description": "Username of the user"
          },
          "team_id": {
            "type": "string",
            "description": "ID of the ClickUp team"
          },
          "space_id": {
            "type": "string",
            "description": "ID of the ClickUp space"
          },
          "folder_id": {
            "type": "string",
            "nullable": true,
            "description": "ID of the ClickUp folder"
          },
          "list_id": {
            "type": "string",
            "description": "ID of the ClickUp task list"
          },
          "auto_sync": {
            "type": "boolean",
            "description": "Enable automatic synchronization"
          },
          "sync_interval": {
            "type": "integer",
            "description": "Sync interval in minutes",
            "minimum": 1,
            "maximum": 1440
          },
          "path_description": {
            "type": "string",
            "readOnly": true,
            "description": "Human-readable path description"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "readOnly": true
          },
          "updated_at": {
            "type": "string",
            "format": "date-time",
            "readOnly": true
          },
          "last_sync_at": {
            "type": "string",
            "format": "date-time",
            "nullable": true,
            "readOnly": true
          }
        },
        "required": ["user", "team_id", "space_id", "list_id", "auto_sync", "sync_interval"]
      },
      "ClickUpSettingsUpdate": {
        "type": "object",
        "properties": {
          "team_id": {
            "type": "string",
            "description": "ID of the ClickUp team"
          },
          "space_id": {
            "type": "string",
            "description": "ID of the ClickUp space"
          },
          "folder_id": {
            "type": "string",
            "description": "ID of the ClickUp folder"
          },
          "list_id": {
            "type": "string",
            "description": "ID of the ClickUp task list"
          },
          "auto_sync": {
            "type": "boolean",
            "description": "Enable automatic synchronization"
          },
          "sync_interval": {
            "type": "integer",
            "description": "Sync interval in minutes",
            "minimum": 1,
            "maximum": 1440
          }
        },
        "required": ["team_id", "space_id", "list_id", "auto_sync", "sync_interval"]
      },
      "ClickUpTask": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "task_id": {
            "type": "string",
            "description": "Unique ID of the task in ClickUp"
          },
          "name": {
            "type": "string",
            "description": "Task name"
          },
          "description": {
            "type": "string",
            "description": "Task description"
          },
          "status": {
            "type": "string",
            "description": "Task status"
          },
          "priority": {
            "type": "string",
