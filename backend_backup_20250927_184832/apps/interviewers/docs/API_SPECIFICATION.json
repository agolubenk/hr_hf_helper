    "/api/interview-rules/{id}/check-grade/": {
      "post": {
        "tags": ["Interview Rules"],
        "summary": "Проверка грейда",
        "description": "Проверка, подходит ли грейд для правила",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID правила",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "grade_id": {
                    "type": "integer",
                    "description": "ID грейда для проверки"
                  }
                },
                "required": ["grade_id"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Результат проверки",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "grade_name": {
                      "type": "string"
                    },
                    "rule_name": {
                      "type": "string"
                    },
                    "is_in_range": {
                      "type": "boolean"
                    },
                    "grade_range": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Ошибка валидации",
