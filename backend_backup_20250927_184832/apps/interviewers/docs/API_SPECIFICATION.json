{
  "openapi": "3.0.0",
  "info": {
    "title": "Interviewers API",
    "description": "API для управления интервьюерами и правилами привлечения",
    "version": "1.0.0",
    "contact": {
      "name": "HR Helper API Support",
      "email": "support@hrhelper.com"
    }
  },
  "servers": [
    {
      "url": "http://localhost:8000",
      "description": "Development server"
    },
    {
      "url": "https://api.hrhelper.com",
      "description": "Production server"
    }
  ],
  "security": [
    {
      "TokenAuth": []
    }
  ],
  "paths": {
    "/api/interviewers/": {
      "get": {
        "tags": ["Interviewers"],
        "summary": "Список интервьюеров",
        "description": "Получение списка всех интервьюеров с поддержкой пагинации, фильтрации и поиска",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "Номер страницы",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "default": 1
            }
          },
          {
            "name": "page_size",
            "in": "query",
            "description": "Размер страницы",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "maximum": 100,
              "default": 20
            }
          },
          {
            "name": "search",
            "in": "query",
            "description": "Поиск по имени, фамилии, email",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "is_active",
            "in": "query",
            "description": "Фильтр по активности",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "ordering",
            "in": "query",
            "description": "Сортировка",
            "schema": {
              "type": "string",
              "enum": ["last_name", "-last_name", "first_name", "-first_name", "email", "-email", "created_at", "-created_at"]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Список интервьюеров",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "count": {
                      "type": "integer",
                      "description": "Общее количество интервьюеров"
                    },
                    "next": {
                      "type": "string",
                      "nullable": true,
                      "description": "URL следующей страницы"
                    },
                    "previous": {
                      "type": "string",
                      "nullable": true,
                      "description": "URL предыдущей страницы"
                    },
                    "results": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Interviewer"
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      },
      "post": {
        "tags": ["Interviewers"],
        "summary": "Создание интервьюера",
        "description": "Создание нового интервьюера",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InterviewerCreate"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Интервьюер создан",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Interviewer"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          }
        }
      }
    },
